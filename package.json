{
  "name": "tc-vform-maker",
  "displayName": "TC VForm Maker",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tc-vform.openTcVForm",
        "title": "Open TC VForm Designer",
        "shortTitle": "打开tc vform设计器"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "tc-vform.openTcVForm",
          "group": "tc-group",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.40.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aimmarc/tc-vform-maker"
  }
}